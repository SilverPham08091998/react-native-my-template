{
  "name": "rntemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "android-debug-develop": " yarn android-clean  && export ENVFILE=.env.dev && react-native run-android --variant=DEVDebug --appId 'com.vn.rntemplate.dev'",
    "android-debug-qa": "yarn android-clean  && export ENVFILE=.env.qa && react-native run-android --variant=QADebug --appId 'com.vn.rntemplate.qa'",
    "android-debug-uat": "yarn android-clean  && export ENVFILE=.env.uat && react-native run-android --variant=UATDebug --appId 'com.vn.rntemplate.uat'",
    "android-debug-production": "yarn android-clean  && export ENVFILE=.env.production && react-native run-android --variant=PRODUCTIONDebug --appId 'com.vn.rntemplate'",
    "android-release-develop": "yarn android-clean  && export ENVFILE=.env.dev && react-native run-android --variant=DEVRelease --appId 'com.vn.rntemplate.dev'",
    "android-release-qa": "yarn android-clean  && export ENVFILE=.env.qa && react-native run-android --variant=QARelease --appId 'com.vn.rntemplate.qa'",
    "android-release-uat": "yarn android-clean  && export ENVFILE=.env.uat && react-native run-android --variant=UATRelease --appId 'com.vn.rntemplate.uat'",
    "android-release-production": "yarn android-clean  && export ENVFILE=.env.production && react-native run-android --variant=PRODUCTIONRelease --appId 'com.vn.rntemplate'"
  },
